<p id="notice"><%= notice %></p>

<h1>Judges</h1>

<table>
  <thead>
    <tr>
      <th>Judge firstname</th>
      <th>Judge lastname</th>
      <th>Issitting</th>
      <th>Judge biography</th>
      <th>Judge email</th>
      <th>Judge polaff</th>
      <th>Ballot</th>
      <th>Precinct</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @judges.each do |judge| %>
      <tr>
        <td><%= judge.judge_firstname %></td>
        <td><%= judge.judge_lastname %></td>
        <td><%= judge.isSitting %></td>
        <td><%= judge.judge_biography %></td>
        <td><%= judge.judge_email %></td>
        <td><%= judge.judge_polaff %></td>
        <td><%= judge.ballot %></td>
        <td><%= judge.precinct %></td>
        <td><%= link_to 'Show', judge %></td>
        <td><%= link_to 'Edit', edit_judge_path(judge) %></td>
        <td><%= link_to 'Destroy', judge, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Judge', new_judge_path %>
