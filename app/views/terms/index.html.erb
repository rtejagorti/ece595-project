<p id="notice"><%= notice %></p>

<h1>Terms</h1>

<table>
  <thead>
    <tr>
      <th>Position title</th>
      <th>Termstart</th>
      <th>Termend</th>
      <th>Candidate</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @terms.each do |term| %>
      <tr>
        <td><%= term.position_title %></td>
        <td><%= term.termStart %></td>
        <td><%= term.termEnd %></td>
        <td><%= term.candidate %></td>
        <td><%= link_to 'Show', term %></td>
        <td><%= link_to 'Edit', edit_term_path(term) %></td>
        <td><%= link_to 'Destroy', term, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Term', new_term_path %>
