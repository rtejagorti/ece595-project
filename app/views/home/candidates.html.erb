<h1>Candidates</h1>
<p>Select a candidate&hellip;</p>
<table class="detail-table candidate-table">
    <thead>
        <tr>
            <th>Last Name</th>
            <th>First Name</th>
            <th>Party</th>
            <th>Encumbent</th>
            <th>Precinct</th>
            <th>E-Mail</th>
            <th>Website</th>
            <th>Details</th>
        </tr>
    </thead>
    <tbody>
        <tr>
            <td>{{last_name}}</td>
            <td>{{first_name}}</td>
            <td>{{pol_aff}}</td>
            <td>{{isEncumbent}}</td>
            <td>{{precinct_id}}</td>
            <td>{{email}}</td>
            <td>{{pol_URL}}</td>
            <td><a href="{{this_will_link_to_candidate}}">More&hellip;</a></td>
        </tr>
        <tr>
            <td>{{last_name}}</td>
            <td>{{first_name}}</td>
            <td>{{pol_aff}}</td>
            <td>{{isEncumbent}}</td>
            <td>{{district_id}}</td>
            <td>{{email}}</td>
            <td>{{pol_URL}}</td>
            <td><a href="{{this_will_link_to_candidate}}">More&hellip;</a></td>
        </tr>
    </tbody>
</table>
